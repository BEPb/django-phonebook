# Описание команда mySQL
# показать все базы данных
SHOW DATABASES;


CREATE TABLE phone (
  ID INT(12) NOT NULL AUTO_INCREMENT,
  DEPARTMENT INT(12) NOT NULL,
  SEQUENCE SMALLINT(6) NOT NULL,
  TITLE VARCHAR(50) NOT NULL,
  RANK VARCHAR(10) NULL,
  EMPLOYEE VARCHAR(50) NULL,
  SERVICE BLOB NULL,
  PERSONAL BLOB NULL,
  EMAIL BLOB NULL,
  BIRTH DATE NULL,
  HIDE TINYINT(1) NOT NULL,
  PRIMARY KEY (ID)
);
- Описание для таблицы phone
CREATE TABLE phone (
  ID INT(12) UNSIGNED NOT NULL AUTO_INCREMENT,
  DEPARTMENT INT(12) UNSIGNED NOT NULL,
  SEQUENCE SMALLINT(6) UNSIGNED NOT NULL,
  TITLE VARCHAR(50) NULL,
  RANK VARCHAR(10) NULL,
  EMPLOYEE VARCHAR(50) NULL,
  SERVICE BLOB NULL,
  PERSONAL BLOB NULL,
  EMAIL BLOB NULL,
  BIRTH DATE NULL,
  HIDE TINYINT(1) NOT NULL,
  PRIMARY KEY (ID),
  CONSTRAINT FK_phone_department_ID FOREIGN KEY (DEPARTMENT)
    REFERENCES department(ID) ON DELETE CASCADE ON UPDATE CASCADE
)
ENGINE = INNODB
AUTO_INCREMENT = 3313
AVG_ROW_LENGTH = 174
CHARACTER SET utf8
COLLATE utf8_general_ci
COMMENT = '';


LOAD DATA INFILE 'C:\pyProject\Django-sites\first\phonebooke_site\phone.csv'
INTO TABLE discounts
FIELDS TERMINATED BY ','
ENCLOSED BY '"'
LINES TERMINATED BY 'n'
IGNORE 1 ROWS;


CREATE TABLE phone (
  ID INT(12) UNSIGNED NOT NULL AUTO_INCREMENT,
  DEPARTMENT INT(12) NOT NULL
);

